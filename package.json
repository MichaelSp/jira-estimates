{
  "name": "jira-estimates",
  "version": "v1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "package": "ncc build --source-map --license licenses.txt && dos2unix dist/*",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelSp/jira-estimates.git"
  },
  "keywords": [
    "actions",
    "github",
    "jira",
    "estimate",
    "story points"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "2.0.1",
    "@actions/github": "6.0.1",
    "jira-client": "8.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@octokit/core": "7.0.6",
    "@octokit/plugin-rest-endpoint-methods": "17.0.0",
    "@types/jest": "30.0.0",
    "@types/jira-client": "7.1.9",
    "@types/node": "25.0.2",
    "@vercel/ncc": "0.38.4",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "js-yaml": "4.1.1",
    "prettier": "3.7.4",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
