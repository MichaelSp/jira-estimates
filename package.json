{
  "name": "jira-estimates",
  "version": "v1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt && dos2unix dist/*",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelSp/jira-estimates.git"
  },
  "keywords": [
    "actions",
    "github",
    "jira",
    "estimate",
    "story points"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "jira-client": "8.2.2"
  },
  "devDependencies": {
    "@octokit/core": "6.0.1",
    "@octokit/plugin-rest-endpoint-methods": "11.0.1",
    "@types/jest": "29.5.12",
    "@types/jira-client": "7.1.9",
    "@types/node": "20.11.25",
    "@typescript-eslint/parser": "7.1.1",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.57.0",
    "eslint-plugin-github": "4.10.2",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.4.2"
  }
}
