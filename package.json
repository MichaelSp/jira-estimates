{
  "name": "jira-estimates",
  "version": "v1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt && dos2unix dist/*",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelSp/jira-estimates.git"
  },
  "keywords": [
    "actions",
    "github",
    "jira",
    "estimate",
    "story points"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/github": "5.0.3",
    "jira-client": "8.1.0"
  },
  "devDependencies": {
    "@octokit/core": "3.6.0",
    "@octokit/plugin-rest-endpoint-methods": "5.13.0",
    "@types/jest": "28.1.1",
    "@types/jira-client": "7.1.4",
    "@types/node": "17.0.38",
    "@typescript-eslint/parser": "5.27.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.16.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.2.2",
    "jest": "28.1.0",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.2"
  }
}
